                                <RowDefinition Height="171*"/>
                                <RowDefinition Height="236*"/>
                            </Grid.RowDefinitions>
                            <Label x:Name="lblNombreVoluntario" Content="Nombre:" Height="26" Margin="95,18,0,0" Width="65" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                            <TextBox x:Name="txtboxNombreVoluntario" HorizontalAlignment="Left" Height="22"  TextWrapping="Wrap" VerticalAlignment="Top" Width="119" FontSize="13" Margin="164,18,0,0" KeyDown="txtboxNombreVoluntario_KeyDown"/>
                            <Label x:Name="lblApellido" Content="Apellido:" HorizontalAlignment="Left" Height="26" Margin="95,49,0,0" VerticalAlignment="Top" Width="64" FontSize="14"/>
                            <TextBox x:Name="txtboxApellidoVoluntario" HorizontalAlignment="Left" Height="22" Margin="164,49,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="202" FontSize="13" RenderTransformOrigin="2,0.509" KeyDown="txtboxApellidoVoluntario_KeyDown"/>
                            <Label x:Name="lblDNIVoluntario" Content="DNI:" HorizontalAlignment="Left" Height="26" Margin="120,80,0,0" VerticalAlignment="Top" Width="44" FontSize="14"/>
                            <TextBox x:Name="txtboxDNIVoluntario" HorizontalAlignment="Left" Height="22" Margin="164,80,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="90" FontSize="13" KeyDown="txtboxDNIVoluntario_KeyDown" MaxLength="9"/>
                            <Label x:Name="lblTelefonoVoluntario" Content="Teléfono de contacto:" HorizontalAlignment="Left" Height="26" Margin="17,111,0,0" VerticalAlignment="Top" Width="144" FontSize="14"/>
                            <Label x:Name="lblCorreoElectronicoVoluntario" Content="Correo electrónico:" HorizontalAlignment="Left" Height="26" Margin="32,142,0,0" VerticalAlignment="Top" Width="127" FontSize="14"/>
                            <Label x:Name="lblConoVeterinarios" Content="Conocimientos veterinarios:" HorizontalAlignment="Left" Height="29" Margin="15,171,0,0" VerticalAlignment="Top" Width="185" FontSize="14" Grid.RowSpan="2"/>
                            <TextBox x:Name="txtboxTelefonoVoluntario" FontSize="13" MaxLength="9" HorizontalAlignment="Left" Height="22" Margin="164,111,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="90" RenderTransformOrigin="2,0.509" KeyDown="txtboxTelefonoVoluntario_KeyDown"/>
                            <TextBox x:Name="txtCorreoElectronicoVoluntario" HorizontalAlignment="Left" Height="22" Margin="164,142,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="170" FontSize="13"/>
                            <Button x:Name="btnAñadirImagenVoluntario" Content="Añadir Imagen" HorizontalAlignment="Left" Height="20" Margin="398,120,0,0" VerticalAlignment="Top" Width="70" Click="btnAñadirImagen_Click" FontSize="9" />
                            <CheckBox x:Name="checkConocimientosVeterinarios" HorizontalAlignment="Left" Height="17" Margin="205,176,0,0" VerticalAlignment="Top" Width="20" RenderTransformOrigin="0.36,-1.306" Grid.RowSpan="2"/>
                            <GroupBox Header="Disponibilidad" Height="193" Margin="10,46,280,0" VerticalAlignment="Top" FontSize="14" Grid.Row="1" BorderBrush="Blue" BorderThickness="2">
                                <Grid HorizontalAlignment="Left" Height="173" Margin="-3,-8,-2,0" VerticalAlignment="Top" Width="563">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>

                                    <CheckBox x:Name="CheckBoxLunes" Content="Lunes" HorizontalAlignment="Left" Height="23" Margin="10,15,0,0" VerticalAlignment="Top" Width="70" Checked="CheckBoxLunes_Checked" Unchecked="CheckBoxLunes_Unchecked"/>
                                    <CheckBox x:Name="CheckBoxMartes" Content="Martes" HorizontalAlignment="Left" Height="23" Margin="10,35,0,0" VerticalAlignment="Top" Width="70" Checked="CheckBoxMartes_Checked" Unchecked="CheckBoxMartes_Unchecked"/>
                                    <CheckBox x:Name="CheckBoxMiercoles" Content="Miercoles" HorizontalAlignment="Left" Height="23" Margin="10,55,0,0" VerticalAlignment="Top" Width="84" Checked="CheckBoxMiercoles_Checked" Unchecked="CheckBoxMiercoles_Unchecked"/>
                                    <CheckBox x:Name="CheckBoxJueves" Content="Jueves" HorizontalAlignment="Left" Height="23" Margin="10,75,0,0" VerticalAlignment="Top" Width="70" Checked="CheckBoxJueves_Checked" Unchecked="CheckBoxJueves_Unchecked"/>
                                    <CheckBox x:Name="CheckBoxViernes" Content="Viernes" HorizontalAlignment="Left" Height="23" Margin="10,95,0,0" VerticalAlignment="Top" Width="70" Checked="CheckBoxViernes_Checked" Unchecked="CheckBoxViernes_Unchecked"/>
                                    <CheckBox x:Name="CheckBoxSabado" Content="Sábado" HorizontalAlignment="Left" Height="23" Margin="10,115,0,0" VerticalAlignment="Top" Width="70" Checked="CheckBoxSabado_Checked" Unchecked="CheckBoxSabado_Unchecked"/>
                                    <CheckBox x:Name="CheckBoxDomingo" Content="Domingo" HorizontalAlignment="Left" Height="23" Margin="10,135,0,0" VerticalAlignment="Top" Width="84" Checked="CheckBoxDomingo_Checked" Unchecked="CheckBoxDomingo_Unchecked"/>
                                    <GroupBox HorizontalAlignment="Left" Height="163" VerticalAlignment="Top" Width="173" Margin="99,10,0,0" BorderBrush="Blue" BorderThickness="2">
                                        <Grid HorizontalAlignment="Left" Height="163" VerticalAlignment="Top" Width="137" RenderTransformOrigin="0,0.812" Margin="10,0,0,-13.4" >
                                            <CheckBox x:Name="CheckBoxLunesMa" Content="Mañana" HorizontalAlignment="Left" Height="23" Margin="0,5,0,0" VerticalAlignment="Top" Width="70" Checked="CheckBoxLunes_Checked" RenderTransformOrigin="0.447,0.887" IsEnabled="False"/>
                                            <CheckBox x:Name="CheckBoxMartesMa" Content="Mañana" HorizontalAlignment="Left" Height="23" Margin="0,25,0,0" VerticalAlignment="Top" Width="70" RenderTransformOrigin="0.956,0.022" IsEnabled="False"/>
                                            <CheckBox x:Name="CheckBoxMiercolesMa" Content="Mañana" HorizontalAlignment="Left" Height="23" Margin="0,45,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False"/>
                                            <CheckBox x:Name="CheckBoxJuevesMa" Content="Mañana" HorizontalAlignment="Left" Height="23" Margin="0,65,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False"/>
                                            <CheckBox x:Name="CheckBoxViernesMa" Content="Mañana" HorizontalAlignment="Left" Height="23" Margin="0,85,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False"/>
                                            <CheckBox x:Name="CheckBoxSabadoMa" Content="Mañana" HorizontalAlignment="Left" Height="23" Margin="0,105,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False"/>
                                            <CheckBox x:Name="CheckBoxDomingoMa" Content="Mañana" HorizontalAlignment="Left" Height="23" Margin="0,125,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False"/>
                                            <CheckBox x:Name="CheckBoxLunesTar" Content="Tarde" HorizontalAlignment="Left" Height="23" Margin="80,5,0,0" VerticalAlignment="Top" Width="70" Checked="CheckBoxLunes_Checked" RenderTransformOrigin="0.447,0.887" IsEnabled="False"/>
                                            <CheckBox x:Name="CheckBoxMartesTar" Content="Tarde" HorizontalAlignment="Left" Height="23" Margin="80,25,0,0" VerticalAlignment="Top" Width="70" RenderTransformOrigin="0.956,0.022" IsEnabled="False"/>
                                            <CheckBox x:Name="CheckBoxMiercolesTar" Content="Tarde" HorizontalAlignment="Left" Height="23" Margin="80,45,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False"/>
                                            <CheckBox x:Name="CheckBoxJuevesTar" Content="Tarde" HorizontalAlignment="Left" Height="23" Margin="80,65,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False"/>
                                            <CheckBox x:Name="CheckBoxViernesTar" Content="Tarde" HorizontalAlignment="Left" Height="23" Margin="80,85,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False"/>
                                            <CheckBox x:Name="CheckBoxSabadoTar" Content="Tarde" HorizontalAlignment="Left" Height="23" Margin="80,105,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False"/>
                                            <CheckBox x:Name="CheckBoxDomingoTar" Content="Tarde" HorizontalAlignment="Left" Height="23" Margin="80,125,0,0" VerticalAlignment="Top" Width="70" IsEnabled="False"/>
                                        </Grid>
                                    </GroupBox>


                                </Grid>

                            </GroupBox>

                            <Border x:Name="bordeImgVoluntario" BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="89" Margin="384,15,0,0" VerticalAlignment="Top" Width="97">
                                <Image x:Name="imgVoluntario" HorizontalAlignment="Center" Height="69" VerticalAlignment="Top" Width="77" Margin="9.2,9.2,9,0"/>
                            </Border>




                        </Grid>
                    </GroupBox>
                    <Button x:Name="btnListarVoluntario" Content="Listar" HorizontalAlignment="Left" Height="46" Margin="569,350,0,0" VerticalAlignment="Top" Width="123" FontSize="14" Click="btnListar_Click" />
                    <Button x:Name="btnAñadirVoluntario" Content="Añadir" HorizontalAlignment="Left" Height="46" Margin="775,350,0,0" VerticalAlignment="Top" Width="123" FontSize="14" Click="btnAñadir_Click"/>
                    <Button x:Name="btnBorrarVoluntario" Content="Borrar" HorizontalAlignment="Left" Height="46" Margin="775,416,0,0" VerticalAlignment="Top" Width="123" FontSize="14"/>
                    <Button x:Name="btnEditarVoluntario" Content="Editar" HorizontalAlignment="Left" Height="46" Margin="569,416,0,0" VerticalAlignment="Top" Width="123" FontSize="14"/>
                    <DataGrid x:Name="dataGridVoluntarios" HorizontalAlignment="Left" Height="277" Margin="534,23,0,0" VerticalAlignment="Top" Width="420" SelectionChanged="dataGridPerros_SelectionChanged">
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="Nombre" Binding="{Binding nombre}" ClipboardContentBinding="{x:Null}"/>
                            <DataGridTextColumn Header="Apellido" Binding="{Binding apellido}" ClipboardContentBinding="{x:Null}"/>
                            <DataGridTextColumn Header="DNI" Binding="{Binding dni}" ClipboardContentBinding="{x:Null}"/>
                            <DataGridTextColumn Header="Telefono" Binding="{Binding n_tlf}" ClipboardContentBinding="{x:Null}"/>
                            <DataGridTextColumn Header="Correo Electronico" Binding="{Binding correo}" ClipboardContentBinding="{x:Null}"/>
                            <DataGridTextColumn Header="¿Conocimientos Veterinarios?" Binding="{Binding conocimientos}" ClipboardContentBinding="{x:Null}"/>
                            <DataGridTextColumn Header="Disponibilidad" Binding="{Binding disponibilidad}" ClipboardContentBinding="{x:Null}"/>
                            <DataGridTextColumn Header="Imagen" Binding="{Binding img}" ClipboardContentBinding="{x:Null}"/>
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
            </TabItem>
            <TabItem x:Name="tabSocios" Header="Gestión de socios" FontFamily="Open Sans Semibold" >
                <Grid x:Name="gridSocios" HorizontalAlignment="Left" Height="505" VerticalAlignment="Top" Width="985" Margin="-2,0,-1,-2">
                    <GroupBox x:Name="grpDatosSocio" Header="Datos del socio" Margin="0,23,462,24" HorizontalAlignment="Right" Width="513" BorderBrush="Orange" BorderThickness="2" FontSize="18">
                        <Grid x:Name="gridDatosSocio" HorizontalAlignment="Left" Height="429" Margin="4,3,0,0" VerticalAlignment="Top" Width="582">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="171*"/>
                                <RowDefinition Height="236*"/>
